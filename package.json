{
  "name": "n8n-nodes-servicetitan",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for ServiceTitan providing 17 resources and 100+ operations for field service management including customers, jobs, appointments, invoicing, dispatch, and technician management.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-servicetitan.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-servicetitan#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-servicetitan/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "servicetitan",
    "field-service",
    "home-services",
    "hvac",
    "plumbing",
    "electrical",
    "dispatch",
    "scheduling",
    "invoicing",
    "crm",
    "automation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ServiceTitanApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ServiceTitan/ServiceTitan.node.js",
      "dist/nodes/ServiceTitan/ServiceTitanTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "copyfiles -u 2 \"nodes/**/*.svg\" \"nodes/**/*.png\" dist/",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
